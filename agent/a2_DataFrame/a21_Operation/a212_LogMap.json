{
    "Access": {
        "Memo": "대상: Core",
        "Log": {
            "Access": "{Info} 🕐 {Timestamp} >>> 👋[((Access)) | {Email} | {ProjectName}]🤘",
            "Complete": "{Info} 🕐 {Timestamp} >>> 👏[((Complete)) | {Email} | {ProjectName}]👍"
        },
        "Info": {
            "Hello": "((🤗 Hello))",
            "Goodbye": "((🥳 Goodbye))"
        }
    },
    "Solution": {
        "Memo": "대상: Solution",
        "Log": {
            "Core": "{Info} 🕐 {Timestamp} >> 🔴[(Core) | {ProjectName} | {Solution}({NextSolution})]🔴",
            "Solution": "{Info} 🕐 {Timestamp} >> 🟡[(Solution) | {ProjectName} | {Solution}({NextSolution}) | {ProcessNumber}_{ProcessName}]🟡",
            "Process": "{Info} 🕐 {Timestamp} >> 🔵[(Process) | {ProjectName} | {Solution}({NextSolution}) | {ProcessNumber}_{ProcessName}]🔵",
            "Task": "{Info} 🕐 {Timestamp} >> 🟣[(Task) | {Solution}({NextSolution}) | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength})]🟣"
        },
        "Info": {
            "Start": "(⚡️ Start)",
            "Complete": "(✅ Complete)",
            "Skip": "(☑️ Skip)",
            "Stop": "(⛔️ Stop)"
        }
    },
    "Process": {
        "Memo": "대상: Agent, LLM",
        "Log": {
            "InputsPreprocess": "(⭐️ Run) 🕐 {Timestamp} > 🟥(InputPreprocess | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength}))🟥\n└─{FunctionName} > {Info}",
            "Inputs": "(⭐️ Run) 🕐 {Timestamp} > 🟨(Inputs | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength}))🟨\n└─{FunctionName} > {Info}",
            "Message": "(⭐️ Run) 🕐 {Timestamp} > 🟦(Message | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength}))🟦\n└─{FunctionName} > {Info}",
            "ResponsePreprocess": "(⭐️ Run) 🕐 {Timestamp} > 🟫(ResponsePreprocess | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength}))🟫\n└─{FunctionName} > {Info}",
            "ResponsePostprocess": "(⭐️ Run) 🕐 {Timestamp} > 🟪(ResponsePostprocess | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength}))🟪\n└─{FunctionName} > {Info}",
            "MiddleFrame": "(⭐️ Run) 🕐 {Timestamp} > 🟩(MiddleFrame | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength}))🟩\n└─{FunctionName} > {Info}",
            "Outputs": "(⭐️ Run) 🕐 {Timestamp} > 🟧(Outputs | {ProcessNumber}_{ProcessName}({InputCount}/{InputCountLength}))🟧\n└─{FunctionName} > {Info}",
            "Function": "└─{FunctionName} > {Info}"
        },
        "Info": {
            "Complete": "✅ Complete: {Print}",
            "Create": "🆕 Create: {Print}",
            "Update": "🔄 Update: {Print}",
            "Load": "🔄 Load: {Print}",
            "Notice": "✔️ Notice: {Print}",
            "Message": "💬 Message\n\n{Print}",
            "Manual": "👉 Manual\n\n{Print}",
            "Error": "❗️ Error: {Print}"
        }
    },
    "Function": {
        "Memo": "대상: 모든 함수",
        "Log": {
            "Function": "└───{FunctionName} > {Info}"
        },
        "Info": {
            "Notice": "Notice: {Print}",
            "Error": "Error: {Print}"
        }
    }
